    <meta name="csrf-token" content="{{xsrfToken}}">
    <meta name="Description" CONTENT="greenscape nursery is a best fruit, flower, plant wholesale supplier in kadiyam, it also offers gardening and land scape services">
    <meta name="keywords" content="greenscape, nursery, kadiyam, plants, rajahmundry, kakinada, online, shopping, flower, fruits, trees, wholesale, supplier, gardening, landscapers">
    <meta name=”robots” content="index, follow">
    <meta charset="UTF-8">
    <title>GreenScape Nursery.in kadiyam</title>
    <link rel="icon" href="/static/img/logo.jpg" type="image/jpg" sizes="16x16">
    <link rel="stylesheet" href="/node_modules/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="/node_modules/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="/node_modules/bootstrap-select/dist/css/bootstrap-select.min.css">
    <link rel="stylesheet" type="text/css" href="/static/css/jquery.dataTables.min.css">
    <link rel="stylesheet" href="/node_modules/bootstrap-icons/font/bootstrap-icons.css">
    <!-- custom css-->
    <link rel="stylesheet" href="/static/css/star-rating-svg.css">
    <link rel="stylesheet" href="/static/css/social_media.css">
    <link rel="stylesheet" href="/static/css/style.css">
    <link rel="stylesheet" href="/static/css/font.css">
    <link rel="stylesheet" href="/static/css/backgrounds.css">



        <!-- jQuery library -->
    <script type="text/javascript" src="/node_modules/jquery/dist/jquery.min.js"></script>

    <!-- Popper JS -->
    <script type="text/javascript" src="/node_modules/popper.js/dist/umd/popper.min.js"></script>
    
    <!-- Latest compiled JavaScript -->
    <script type="text/javascript" src="/node_modules/bootstrap/dist/js/bootstrap.min.js"></script>


    <!-- Latest compiled and minified JavaScript -->
    <script type="text/javascript" src="/node_modules/bootstrap-select/dist/js/bootstrap-select.min.js"></script>

    
    <script type="text/javascript" src="/node_modules/datatables.net/js/jquery.dataTables.min.js"></script>

    <script type="text/javascript" src="/node_modules/datatables.net-plugins/dataRender/datetime.js"></script>

